/* pages/shopProfitDetail/shopProfitDetail.wxss */
.gs_tabs{
  position: sticky;
  position: -webkit-sticky;
  left: 0;
  top: 0;
  z-index: 120;
}
.gs_t_item {
    height: 96rpx;
    min-width: 50%;
    /*color: #0*/
}
.gs_t_item text {
    height: 72rpx;
    line-height: 72rpx;
    text-align: center;
    font-size: 28rpx;
    width: 80%;
}
.spd_sum {
    padding: 48rpx 0;
}
.spd_sum img {
    width: 48rpx;
    height: 48rpx;
}
.saleOut{
    width: 96rpx;
    padding: 4.8rpx;
    color: #fed78a !important;
    border-radius: 24rpx;
    border: 2px solid #fed78a;
  }

  /* 遮罩层 */
.mask{
    width:100%;
    height:100%;
    position:fixed;
    top:0;
    left:0;
    background:#000;
    z-index:9000;
    opacity:0.5;
  }
  /* 弹出层 */

  .order_pay {
    position: fixed;
    bottom: 0;
    z-index: 9999;
  }
  
  .order_pay image {
    width: 48rpx;
    height: 48rpx;
  }
  
  .order_pay .common_submit_btn {
    width: 90%;
    border-radius: 48rpx;
  }
  
  .order_pay .op_close {
    position: absolute;
    right: 24rpx;
    top: 14.4rpx;
  }
  
  .op_title text {
    width: 48rpx;
    height: 48rpx;
    background: red;
    border-radius: 50%;
  }
  
  .op_price {
    border-bottom: 2px dashed #efefef;
  }
  .padding_custom {
    padding: 24rpx 36rpx;
  }
  
  
  .dialogModel2{
    width:90%;
    position:fixed;
    top: 0;
    left: 5%;
    margin: auto 0;
    z-index:9999;
    background-color:#fff;
    border-radius:20rpx;
  }